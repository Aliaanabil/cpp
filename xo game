#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    char xo[3][3];

    for(int i=0;i<=2;i++){
            for(int j=0;j<=2;j++){

        cin >> xo[i][j];
    }
    }
     for(int i=0;i<=2;i++){
            for(int j=0;j<=2;j++){

        cout << xo[i][j]<<" ";
    }
    cout<<endl;
     }
    return 0;
}
