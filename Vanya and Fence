#include <iostream>

using namespace std;

int main()
{
   int n;
   double h,sum=0;
   cin >> n >>h;
   int a[100];
   for (int i=1;i<=n;i++)
   {
       cin >> a[i];
   }
   for (int i=1;i<=n;i++)
   {
    if (a[i]<=h){
        sum+=1;
    }
    else{
        sum+=2;
    }
   }
   cout << sum;


    return 0;
}
