#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    string x,y;
    cin >> x;
    cin>>y;
    for(int i=1;i<=x.size();i++){
       if (x[i] < 92) {
            x[i] += 32;
        }
        if (y[i] < 92) {
            y[i] += 32;
        }
    }


        if(x>y){
            cout << "1";
        }
        else if (x<y){
            cout << "-1";
        }
        else{
            cout << "0";
        }



}
